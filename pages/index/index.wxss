/**index.wxss**/

.container {
  background: deepskyblue;
}

.search-box {
  display: flex;
  width: 90%;
  justify-content: space-between;
  border-bottom: 1px solid white;
  background: deepskyblue;
  color: white;
  margin-bottom: 20rpx;
}

.input-placeholder {
  color: white;
  opacity: 0.8;
}

.icon-cancel {
  color: white;
  opacity: 0.8;
}

.btn-group, .class-table {
  background: white;
  width: 100%;
}

.btn-group {
  display: flex;
}

.btn-group button {
  background: black;
  color: white;
  padding: 0 40rpx;
  height: 55rpx;
  line-height: 55rpx;
  margin-top: 20rpx;
  border-radius: 50rpx;
}
